<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Tesis\AdminBundle\Entity\Tutoria" table="tutoria">
    <indexes>
      <index name="fk_gestiona_proyecto1_idx" columns="proyecto_id_proyecto"/>
    </indexes>
    <id name="idTutores" type="integer" column="id_tutores">
      <generator strategy="AUTO"/>
    </id>
    <many-to-one field="proyectoProyecto" target-entity="Proyecto">
      <join-columns>
        <join-column name="proyecto_id_proyecto" referenced-column-name="id_proyecto"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="usuarioUsuario" target-entity="Usuario" inversed-by="tutoriaTutores">
      <join-table name="tutoria_has_usuario">
        <join-columns>
          <join-column name="tutoria_id_tutores" referenced-column-name="id_tutores"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="usuario_id_usuario" referenced-column-name="id_usuario"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>

<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Tesis\AdminBundle\Entity\InfoAdicional" table="info_adicional">
    <indexes>
      <index name="fk_info_adicional_estudiante1_idx" columns="estudiante_id_estudiante"/>
    </indexes>
    <id name="idInfoAdicional" type="integer" column="id_info_adicional">
      <generator strategy="AUTO"/>
    </id>
    <field name="resumen" type="string" column="resumen" length="2000" nullable="true"/>
    <field name="problema" type="string" column="problema" length="2000" nullable="true"/>
    <field name="justificacion" type="string" column="justificacion" length="2000" nullable="true"/>
    <field name="objetivo" type="string" column="objetivo" length="2000" nullable="true"/>
    <field name="resultados" type="string" column="resultados" length="2000" nullable="true"/>
    <field name="conclusion" type="string" column="conclusion" length="2000" nullable="true"/>
    <many-to-one field="estudianteEstudiante" target-entity="Estudiante">
      <join-columns>
        <join-column name="estudiante_id_estudiante" referenced-column-name="id_estudiante"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
